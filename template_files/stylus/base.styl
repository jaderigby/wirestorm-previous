	//
	// Layout (columns/rows)
	//
	.half
		cf()
		> aside
			column(1/2)
			+below(large)
				column(1/1)
		> article
			column(1/2)
			+below(large)
				column(1/1)
	.thirds
		cf()
		> aside
			column(1/3)
			+below(large)
				column(1/1)
		> article
			column(2/3)
			+below(large)
				column(1/1)
	.fourths
		cf()
		> aside
			column(1/4)
			+below(large)
				column(1/1)
		> article
			column(3/4)
			+below(large)
				column(1/1)
	.fifths
		cf()
		> aside
			column(2/5)
			&.single
				column(1/5)
				+below(large)
					column(1/1)
			+below(large)
				column(1/1)
		> article
			column(3/5)
			+below(large)
				column(1/1)
		> aside.single + article
			column(4/5) !important
	
	section.columns-2
		cf()
		article
			column(1/2)
			+below(large)
				column(1/1)

	section.columns-3
