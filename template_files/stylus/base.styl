				height: 11px
				color: #666
				font-size: 0.95rem
				background-color: transparent
				border: 0
	//
	// Forms
	//
	.checkbox-widget
		li.checkbox
			list-style: none
			margin-left: 0
		span.checkbox
			display: inline-block
			margin-bottom: 10px
		span.checkbox
		li.checkbox
			margin-right: 15px
			label
				display: inline-block
			// 	margin-left: 5px
			// &:first-child
			// 	label
			// 		margin-left: 0
			&:last-child
				margin-right: 0
		[type="checkbox"]:not(:checked)
		[type="checkbox"]:checked
			position: absolute
			left: -9999px
		[type="checkbox"]:not(:checked) + label
		[type="checkbox"]:checked + label
			position: relative
			padding-left: 25px
			cursor: pointer
		[type="checkbox"]:not(:checked) + label:before
		[type="checkbox"]:checked + label:before
			content: ''
			position: absolute
			top: 1px
			left: 0 
			width: 14px 
			height: 14px
			border: 1px solid #444
			background: #444
		[type="checkbox"]:not(:checked) + label:after
		[type="checkbox"]:checked + label:after
			content: ''
			position: absolute
			top: 5px 
			left: 4px
			width: 8px
			height: 8px
			background-color: #84f1ff /* 84f1ff */
			transition: all .1s
			-webkit-transition: all .1s
		[type="checkbox"]:not(:checked) + label:after
			opacity: 0
			transform: scale(0)
		[type="checkbox"]:checked + label:after
			opacity: 1
			transform: scale(1)
		[type="checkbox"]:disabled:not(:checked) + label:before,
		[type="checkbox"]:disabled:checked + label:before
			box-shadow: none
			border-color: #bbb
			background-color: textLightOnDark
		[type="checkbox"]:disabled:checked + label:after
			color: #999
		[type="checkbox"]:disabled + label
			color: #aaa
		// accessibility
		[type="checkbox"]:checked:focus + label:before
		[type="checkbox"]:not(:checked):focus + label:before
			border: 1px dotted blue
	.radio-widget
		span.radio
			display: inline-block
			margin-right: 15px
			label
				display: inline-block
			// 	margin-left: 5px
			// &:first-child
			// 	label
			// 		margin-left: 0
			&:last-child
				margin-right: 0
		[type="radio"]:not(:checked)
		[type="radio"]:checked
			position: absolute
			left: -9999px
		[type="radio"]:not(:checked) + label
		[type="radio"]:checked + label
			position: relative
			padding-left: 22px
			cursor: pointer
		[type="radio"]:not(:checked) + label:before
		[type="radio"]:checked + label:before
			content: ''
			position: absolute
			top: 0px
			left:0 
			width: 14px 
			height: 14px
			border-radius: 100px
			border: 1px solid #444
			background: #444
		[type="radio"]:not(:checked) + label:after
		[type="radio"]:checked + label:after
			content: ''
			position: absolute
			top: 4px 
			left: 4px
			width: 8px
			height: 8px
			border-radius: 100px
			background-color: #84f1ff
			transition: all .1s
			-webkit-transition: all .1s
		[type="radio"]:not(:checked) + label:after
			opacity: 0
			transform: scale(0)
		[type="radio"]:checked + label:after
			opacity: 1
			transform: scale(1)
		[type="radio"]:disabled:not(:checked) + label:before,
		[type="radio"]:disabled:checked + label:before
			box-shadow: none
			border-color: #bbb
			background-color: #84f1ff
		[type="radio"]:disabled:checked + label:after
			color: #999
		[type="radio"]:disabled + label
			color: #aaa
		// accessibility
		[type="radio"]:checked:focus + label:before
		[type="radio"]:not(:checked):focus + label:before
			border: 1px dotted blue
	.switch-widget
		ul
			margin: 10px 0
		li
			list-style: none
			cf()
			margin-left: 0
		label
			display: block
			float: left
			margin-top: 4px
			margin-left: 10px
		.switch-object
			float: left
			width: 58px
			height: 20px
			font-size: 0.75rem
			background-color: #444
			border: 2px solid #444
			cursor: pointer
			.switch
				position: absolute
				height: 20px
				width: 29px
				background-color: #CCC
				transition: all 0.03s linear
				-webkit-transition: all 0.05s linear
				z-index: 2
			.on
				position: absolute
				margin-top: 1px
				margin-left: 1px
				width: 25px
				height: 18px
				background-color: #81EAF7 // 81EAF7, 7FE8F6
			.on-icon
				display: block
				position: absolute
				margin-top: 1px
				margin-left: 6px
				color: #81EAF7
				font-size: 1rem
			.on-text
				position: relative
				float: left
				padding-top: 5px
				padding-bottom: 3px
				width: 29px
				color: #84F1FF
				text-align: center
				background-color: transparent
			.off-text
				position: relative
				margin-top: 5px
				float: right
				text-align: right
				width: 29px
				color: textLightOnDark
				text-align: center
			&.on
				.switch
					transform: translateX(29px)
