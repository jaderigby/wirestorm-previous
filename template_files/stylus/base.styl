responsive()
	lg = 1024px
	md = 960px
	tablet = 768px
	sm = 640px
	xs = 320px
	section.break
		cf()
		//
		// FIFTHS
		//
		&.lg-fifths
			> aside
				column(2/5)
			> article
				column(3/5)
			+below(lg)
				> aside
					column(2/5)
				> article
					column(3/5)
		&.md-fifths
			+below(md)
				> aside
					column(2/5)
				> article
					column(3/5)
		&.tablet-fifths
			+below(tablet)
				> aside
					column(2/5)
				> article
					column(3/5)
		&.sm-fifths
			+below(sm)
				> aside
					column(2/5)
				> article
					column(3/5)
		&.xs-fifths
			+below(xs)
				> aside
					column(2/5)
				> article
					column(3/5)
		//
		// FOURTHS
		//
		&.lg-fourths
			> aside
				column(1/4)
			> article
				column(3/4)
			+below(lg)
				> aside
					column(1/4)
				> article
					column(3/4)
		&.md-fourths
			+below(md)
				> aside
					column(1/4)
				> article
					column(3/4)
		&.tablet-fourths
			+below(tablet)
				> aside
					column(1/4)
				> article
					column(3/4)
		&.sm-fourths
			+below(sm)
				> aside
					column(1/4)
				> article
					column(3/4)
		&.xs-fourths
			+below(xs)
				> aside
					column(1/4)
				> article
					column(3/4)
		//	
		// THIRDS
		//
		&.lg-thirds
			> aside
				column(1/3)
			> article
				column(2/3)
			+below(lg)
				> aside
					column(1/3)
				> article
					column(2/3)
		&.md-thirds
			+below(md)
				> aside
					column(1/3)
				> article
					column(2/3)
		&.tablet-thirds
			+below(tablet)
				> aside
					column(1/3)
				> article
					column(2/3)
		&.sm-thirds
			+below(sm)
				> aside
					column(1/3)
				> article
					column(2/3)
		&.xs-thirds
			+below(xs)
				> aside
					column(1/3)
				> article
					column(2/3)
		//	
		// HALF
		//
		&.lg-half
			> aside
				column(1/2)
			> article
				column(1/2)
			+below(lg)
				> aside
					column(1/2)
				> article
					column(1/2)
		&.md-half
			+below(md)
				> aside
					column(1/2)
				> article
					column(1/2)
		&.tablet-half
			+below(tablet)
				> aside
					column(1/2)
				> article
					column(1/2)
		&.sm-half
			+below(sm)
				> aside
					column(1/2)
				> article
					column(1/2)
		&.xs-half
			+below(xs)
				> aside
					column(1/2)
				> article
					column(1/2)
		//
		// STACK
		//
		&.lg-stack
			> aside
				stack()
			> article
				stack()
			+below(lg)
				> aside
					stack()
				> article
					stack()
		&.md-stack
			+below(md)
				> aside
					stack()
				> article
					stack()
		&.tablet-stack
			+below(tablet)
				> aside
					stack()
				> article
					stack()
		&.sm-stack
			+below(sm)
				> aside
					stack()
				> article
					stack()
		&.xs-stack
			+below(xs)
				> aside
					stack()
				> article
					stack()
		//
		// FOR COLUMNS
		//
		for item in (2..10)
			&.lg-columns-{item}
				> article
					column(1/item)
				+below(lg)
					> article
						column(1/item)
			&.md-columns-{item}
				+below(md)
					> article
						column(1/item)
			&.tablet-columns-{item}
				+below(tablet)
					> article
						column(1/item)
			&.sm-columns-{item}
				+below(sm)
					> article
						column(1/item)
			&.xs-columns-{item}
				+below(xs)
					> article
						column(1/item)
			